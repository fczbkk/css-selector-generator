{
  "name": "css-selector-generator",
  "title": "CSS Selector Generator",
  "version": "3.1.0",
  "description": "JavaScript object that creates unique CSS selector for given element.",
  "author": "Riki Fridrich <riki@fczbkk.com> (http://fczbkk.com)",
  "license": "MIT",
  "main": "build/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "karma start ./tools/karma.conf.js",
    "dev": "npm run test -- --no-single-run --auto-watch",
    "prebuild": "npm run lint:build && rimraf ./build",
    "build": "webpack --config ./tools/webpack.build.js",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "eslint ./src/**/*.ts --config ./.eslintrc-typescript.json",
    "lint:js": "eslint ./test/**/*.js ./tools/**/*.js --config ./.eslintrc.json",
    "lint:build": "npm run lint:ts -- --max-warnings 0 && npm run lint:js -- --max-warnings 0",
    "doc": "npm run doc:readme && npm run doc:types",
    "doc:readme": "documentation readme src/index.ts -s 'Documentation' --shallow",
    "doc:types": "tsc ./src/*.ts --declaration --allowJs --emitDeclarationOnly --downlevelIteration --outDir types",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "version": "npm run build && npm run doc && npm run changelog && git add -A"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@fczbkk/eslint-config": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "conventional-changelog-cli": "^2.1.1",
    "documentation": "^13.2.5",
    "eslint": "^7.32.0",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.0.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/css-selector-generator.git"
  },
  "homepage": "https://github.com/fczbkk/css-selector-generator/",
  "dependencies": {
    "cartesian": "^1.0.1",
    "iselement": "^1.1.4"
  }
}
