{
  "extends": "@fczbkk",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": [
      "warn",
      "never"
    ],
    "max-len": [
      "warn",
      {
        "ignoreComments": true
      }
    ]
  },
  "root": true
}
